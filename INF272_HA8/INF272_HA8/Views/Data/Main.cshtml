@model IEnumerable<INF272_HA8.Models.Franchise>

@{
    ViewBag.Title = "Main";
}

<h1 class="text-center"> Maphosa Sokombovu Inc.</h1>
<h4  class="text-center">Section 2</h4>
<div class="row">
    <a class="btn btn-default col-lg-offset-1" href="@Url.Action("CreateETF","Data")">Add New Franchise</a>
    <a class="btn btn-default col-lg-offset-7" href="@Url.Action("Index","Data")">Go To Section 1</a>
</div>
<br />
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FranchiseArea)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FranchiseName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FranchiseOwner)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FranchiseNumberClients)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FranchiseArea)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FranchiseName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FranchiseOwner)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FranchiseNumberClients)
        </td>
        <td>
            @Html.ActionLink("Update", "EditETF", new { id=item.FranchiseID }) |
            @Html.ActionLink("Delete", "DeleteETF", new { id=item.FranchiseID })
        </td>
    </tr>
}

</table>
